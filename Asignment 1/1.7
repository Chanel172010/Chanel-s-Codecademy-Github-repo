#include <iostream>
using namespace std;

int main() {
    cout << "Enter a number" << endl;
    int input;
    cin >> input;

    for (int i = 1; i <= input; i++) {
        for (int j = i; j > 0; j--) {
            int out = j % 2;
            cout << out;
        }
        cout << endl;
    }

    return 0;  
}
